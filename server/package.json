    "@types/supertest": "^6.0.3",
    "jest": "^30.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^2.8.8",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
