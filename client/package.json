{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "description": "Pokemon Battle Simulator with full test suite",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "node run-e2e.js",
    "test:e2e:complete": "testcafe chrome:headless e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:chrome": "testcafe chrome e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:firefox": "testcafe firefox e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:safari": "testcafe safari e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:all": "testcafe chrome,firefox e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:headless": "testcafe chrome:headless e2e/pokemon-battle-complete.test.js --speed 0.1",
    "test:e2e:verify": "node verify-e2e-setup.js",
    "test:all": "npm run test && npm run test:e2e:headless",
    "server": "ts-node src/server/index.ts",
    "server:dev": "nodemon src/server/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "c8": "^8.0.0",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "testcafe": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
